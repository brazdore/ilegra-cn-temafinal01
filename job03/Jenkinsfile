pipeline {

   agent any

   stages {
      stage('Download Image from Docker Hub') {
         steps {
            sh 'docker pull brazdore/ilegra-cn-temafinal01:temafinal01'
         }
      }

      stage('Run Docker Image') {
         steps {
            sh 'docker run -p 8080:8089 -d brazdore/ilegra-cn-temafinal01:temafinal01'
         }
      }
   }
}